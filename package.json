{
  "name": "maria-havens-pos",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --watch server/src --exec ts-node server/src/index.ts",
    "build:frontend": "vite build",
    "build:backend": "cd server && npx tsc",
    "build": "npm run build:frontend && npm run build:backend",
    "preview": "vite preview",
    "start": "node server/dist/index.js",
    "postinstall": "cd server && npm install"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "knex": "^3.0.1",
    "lucide-react": "^0.294.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.3.0",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-pwa": "^1.0.3",
    "ws": "^8.14.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/ws": "^8.5.8",
    "workbox-cli": "^2.1.3"
  }
}
